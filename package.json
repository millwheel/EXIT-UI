{
  "name": "exit-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e dev.env -- next dev",
    "dev:prod": "dotenv -e prod.env -- next dev",
    "build": "dotenv -e prod.env -- prisma generate && dotenv -e prod.env -- next build",
    "start": "dotenv -e prod.env -- next start",
    "lint": "eslint",
    "db:push": "dotenv -e dev.env -- prisma db push",
    "db:push:prod": "dotenv -e prod.env -- prisma db push"
  },
  "dependencies": {
    "@prisma/client": "^6.19.2",
    "bcryptjs": "^3.0.3",
    "jose": "^6.1.3",
    "next": "^16.1.6",
    "prisma": "^6.19.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
